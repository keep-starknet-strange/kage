{
  "expo": {
    "name": "kage",
    "slug": "kage",
    "scheme": "kage",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./kage-logo.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./kage-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B0F10"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./favicon_kit/android-chrome-512x512.png",
        "backgroundColor": "#0B0F10"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./favicon_kit/favicon.ico"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-localization",
      "expo-font"
    ]
  }
}
