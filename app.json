{
  "expo": {
    "name": "kage",
    "slug": "kage",
    "scheme": "kage",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./kage-logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.starknet.kage",
      "appleTeamId": "4M48KC5QKH",
      "infoPlist": {
        "NSFaceIDUsageDescription": "We use Face ID to secure your data"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./favicon_kit/android-chrome-512x512.png",
        "backgroundColor": "#0B0F10"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.starknet.kage"
    },
    "web": {
      "favicon": "./favicon_kit/favicon.ico"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-localization",
      "expo-font",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./favicon_kit/android-chrome-512x512.png",
          "dark": {
            "image": "./favicon_kit/android-chrome-512x512.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ]
    ]
  }
}
